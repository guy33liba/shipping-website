        function pr_ContactCheckEmail(CurMail)
        {
            if (CurMail.length != 0)
            {
                var i=CurMail.indexOf("@")
                var i2=CurMail.indexOf("@",i+1)
                var j=CurMail.indexOf(".",i)
                var k=CurMail.indexOf(",")
                var kk=CurMail.indexOf(" ")
                var jj=CurMail.lastIndexOf(".") + 1
                var leng = CurMail.length

                if ((i>0)&&(j>(i+1))&&(k==-1)&&(kk==-1)&&(leng-jj >= 2)&&(leng-jj <= 3)&&(i2==-1))
                {
                    return true;
                }
                else
                {
                    return false;
                }
            }
            else{
                return false;
            }
        }

