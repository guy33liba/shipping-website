    function redirectToElement(elementId) {
    var element = document.getElementById(elementId);
    if (!element) {
        return false;
    }

    var elementPosition = element.getBoundingClientRect().top + window.pageYOffset;
    
    // Check for sticky header
    var stickyHeader = document.getElementById("sticky_header");
    if (stickyHeader && getComputedStyle(stickyHeader).display !== 'none') {
        elementPosition -= stickyHeader.offsetHeight;
    }

    // Check for slide from top elements
    var slideElements = document.getElementsByClassName('slidefromtop');
    for (var i = 0; i < slideElements.length; i++) {
        if (slideElements[i] && getComputedStyle(slideElements[i]).display !== 'none') {
            elementPosition -= slideElements[i].offsetHeight;
        }
    }

    // Adjust for elements with 'fixed-top' class if they are visible
    var fixedTopElements = document.querySelectorAll("#header.fixed-top, #header .fixed-top");
    fixedTopElements.forEach(function(el) {
        if (getComputedStyle(el).display !== 'none') {
            elementPosition -= el.offsetHeight;
        }
    });

    // Adjust for mobile header if it is visible
    var mobileHeader = document.getElementById("mobile_header");
    if (mobileHeader && getComputedStyle(mobileHeader).display !== 'none') {
        elementPosition -= mobileHeader.offsetHeight;
    }

    window.scrollTo({
        top: elementPosition-30,
        behavior: 'smooth'
    });

    element.focus();
}


