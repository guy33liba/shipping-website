                    function isValidPhoneNumber(phoneNumber) {
                        const length = phoneNumber.length;
                        if (phoneNumber[0] !== '0') {
                            return false;
                        }
                        if (length === 9 && /^0((2|3|4|8|9)[0-9]{7}|(2|3|4|8|9)[0-9]{6})$/.test(phoneNumber)) {
                            return true;
                        }
                        if (length === 10 && /^0((5[0-8]|7[1-9])[0-9]{7}|(2|3|4|8|9))$/.test(phoneNumber)) {
                            return true;
                        }
                        return false;
                    }

